
public class MySchedule 
{
	public static void main( String[] args) 
	{
		String Hist, ECS, English, Span, Lock, Mauro, Leal, Gomez;
		
		Hist = "History";
		ECS = "ECS";
		English = "English";
		Span = "Spanish";
		Lock = "Lockhart";
		Mauro = "Mauro";
		Leal = "Leal";
		Gomez = "Gomez";
		
		System.out.println("--------------------------------------");
		System.out.println("|Per 1  |	" + Hist + "    |    " + Lock + "|" );
		System.out.println("|Per 2  |	" + ECS + "        |    " + Mauro + "|" );
		System.out.println("|Per 3  |	" + English + "    |    " + Leal + "|" );
		System.out.println("|Per 4  |	" + Span + "    |    " + Gomez + "|" );
		
		
		
	}
}
